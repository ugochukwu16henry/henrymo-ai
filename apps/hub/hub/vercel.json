{
  "buildCommand": "cd ../.. && pnpm install && cd apps/hub/hub && pnpm build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "cd ../.. && pnpm install --filter @henrymo-ai/hub",
  "devCommand": "cd ../.. && pnpm dev --filter @henrymo-ai/hub",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}

